(()=>{

 class Manager {
     constructor(
           public Name:string,
            public ID:number,
            public PhoneNo:number,
            public Locacion:string
        ){
          this.Name=Name;
          this.ID=ID;
          this.PhoneNo=PhoneNo;
          this.Locacion= Locacion;  
        }
    }
  const Name: Manager = new Manager(
    `Daniela`,
    1031,
    41235895,
    `Arbolera`);
    console.log(Name);
    console.log (Name.Name);

 class Chef {
     constructor(
